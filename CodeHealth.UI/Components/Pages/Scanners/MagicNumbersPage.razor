@page "/project/{ProjectId}/magic-numbers"

<GenericScanner 
    ProjectId="@ProjectId"
    ScannerName="Magic Numbers"
    ScannerData="@ScannerData"
    ProjectRootDirectory="@ProjectRootDirectory"
    GoBackPage="magic-numbers"
    ValueFormatter="@(r => $"Number: {r.Metric.Value}")"
    BadgeClass="@GetMagicNumberBadgeClass"
    BadgeText="@GetMagicNumberBadgeText" />
